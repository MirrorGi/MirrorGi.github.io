#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

//复杂字符画用printf一行行绘制是笨办法，将字符画存储为字符串数组更方便(其实依旧不是很方便)
int main(void) {
	const char* mario[] = {
		"                ********",
		"               ************",
		"               ####....#.",
		"             #..###.....##....",
		"             ###.......######              ###            ###",
		"                ...........               #...#          #...#",
		"               ##*#######                 #.#.#          #.#.#",
		"            ####*******######             #.#.#          #.#.#",
		"           ...#***.****.*###....          #...#          #...#",
		"           ....**********##.....           ###            ###",
		"           ....****    *****....",
		"             ####        ####",
		"           ######        ######",
		"##############################################################",
		"#...#......#.##...#......#.##...#......#.##------------------#",
		"###########################################------------------#",
		"#..#....#....##..#....#....##..#....#....#####################",
		"##########################################    #----------#",
		"#.....#......##.....#......##.....#......#    #----------#",
		"##########################################    #----------#",
		"#.#..#....#..##.#..#....#..##.#..#....#..#    #----------#",
		"##########################################    ############",
		NULL
	};
	for (int i = 0; mario[i] != NULL; i++) {
		printf("%s\n", mario[i]);
	}

	return 0;
}
